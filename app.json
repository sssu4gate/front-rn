{
  "___comment": "app.json reference (https://docs.expo.io/versions/latest/config/app/)",
  "expo": {
    "name": "4gate",
    "slug": "4gate",
    "version": "0.1.0",
    "primaryColor": "#ff6da0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
